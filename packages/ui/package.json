{
  "name": "@oi/ui",
  "version": "0.0.2",
  "description": "A few extra React components built on top of Material-UI",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "author": "Eric Vera <hey@itsmeeric.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublish": "yarn build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "keywords": [
    "material-ui",
    "react"
  ],
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "prop-types": "^15.7.2",
    "prop-types-exact": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-storyshots": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-loader": "^8.0.5",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "jest": "^24.7.1",
    "react-test-renderer": "^16.8.6"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "<rootDir>/src/.jest/register-context.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/.jest/imageMock.js"
    }
  }
}
